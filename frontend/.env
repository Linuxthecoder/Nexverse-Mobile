# NEXVERSE PRODUCTION CONFIGURATION
# This file is used when deploying to Vercel

# Production Mode
NODE_ENV=development
VITE_APP_MODE=development
VITE_APP_VERSION=1.0.0                                                        

# Backend Configuration (Render)
# VITE_API_URL=https://backend-nexverse-net.onrender.com/api
# VITE_SOCKET_URL=https://backend-nexverse-net.onrender.com
# VITE_BACKEND_URL=https://backend-nexverse-net.onrender.

VITE_API_URL=http://localhost:5001/api
VITE_SOCKET_URL=http://localhost:5001
VITE_BACKEND_URL=http://localhost:5001
                       
# Production Features
VITE_SHOW_DEV_TOOLS=false
VITE_ENABLE_DEBUG_MODE=false
VITE_FORCE_LOCALHOST=false

# Prototype Settings (disabled in production)                   
VITE_PROTOTYPE_MODE=false
VITE_SHOW_PROTOTYPE_BANNER=false
